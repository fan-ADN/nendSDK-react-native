{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "E2E_DEVICE=\"*\" jest --testMatch=\"**/__tests__/**/!(*e2e.js)\"",
    "test:e2e": "jest --testMatch=\"**/__tests__/**/*e2e.js\" --runInBand",
    "test:e2e:android": "E2E_DEVICE=\"android-emulator\" npm run test:e2e",
    "test:e2e:ios": "E2E_DEVICE=\"ios-simulator\"  npm run test:e2e",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "16.9.0",
    "react-native": "0.61.1"
    ,"react-native-gesture-handler": "^1.4.1",
    "react-native-nend-bridger": "file:../react-native-nend-bridger-0.0.1.tgz",
    "react-navigation": "^3.13.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@react-native-community/eslint-config": "0.0.3",
    "appium": "^1.15.0",
    "appium-doctor": "^1.12.0",
    "babel-jest": "24.9.0",
    "eslint": "6.4.0",
    "jest": "24.9.0",
    "jest-jenkins-reporter": "^1.0.2",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.9.0",
    "wd": "^1.11.4"
  },
  "jest": {
    "preset": "react-native"
    ,"testResultsProcessor": "jest-jenkins-reporter"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "report.xml",
    "indent": 4
  }
}
