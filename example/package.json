{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "E2E_DEVICE=\"*\" jest --testMatch=\"**/__tests__/**/!(*e2e.js)\"",
    "test:e2e": "jest --testMatch=\"**/__tests__/**/*e2e.js\"",
    "test:e2e:android": "E2E_DEVICE=\"android-emulator\" npm run test:e2e",
    "test:e2e:ios": "E2E_DEVICE=\"ios-simulator\"  npm run test:e2e"
  },
  "dependencies": {
    "react": "16.8.3",
    "react-native": "0.59.8",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-nend-bridger": "file:../react-native-nend-bridger-0.0.1.tgz",
    "react-navigation": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/runtime": "7.5.5",
    "appium": "^1.14.0",
    "appium-doctor": "^1.11.1",
    "babel-jest": "24.8.0",
    "jest": "24.8.0",
    "jest-jenkins-reporter": "^1.0.2",
    "metro-react-native-babel-preset": "0.55.0",
    "react-test-renderer": "16.8.3",
    "wd": "^1.11.3"
  },
  "jest": {
    "preset": "react-native",
    "testResultsProcessor": "jest-jenkins-reporter"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "report.xml",
    "indent": 4
  }
}
